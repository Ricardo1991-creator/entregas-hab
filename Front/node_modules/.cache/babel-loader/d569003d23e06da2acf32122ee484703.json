n\n<script>\nexport default {\n  name: \"ArtistsCom\",\n  props: {\n    topartists: Array\n  },\n  data() {\n    return {\n      search: \"\"\n    };\n  },\n  computed: {\n    filtered() {\n      if (!this.search) {\n        return this.topartists;\n      }\n      return this.topartists.filter(artist =>\n        artist.name.toLowerCase().includes(this.search.toLowerCase())\n      );\n    }\n  }\n};\n</script>\n\n<style scoped>\nimg {\n  border-radius: 150px;\n}\n</style>"],"sourceRoot":"src/components"}]}